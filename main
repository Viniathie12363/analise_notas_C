#include <stdio.h>

int main(){
    int nota1,nota2,nota3;
    printf("Digite sua primeira nota:\n");
    scanf("%i",&nota1);
    printf("Digite sua segunda nota:\n");
    scanf("%i",&nota2);
    printf("Digite sua terceira nota:\n");
    scanf("%i",&nota3);
    
    float media;
    media = (nota1+nota2+nota3)/3.0;
    if(media>=6){
        printf("Sua nota foi: %f e est√° aprovado",media);
    }

    else{
        printf("Sua nota foi: %f e estpa reprovado",media);
    };
    return 0;
}
